@model List<Profile>
<table class="table">

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProfileUser.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>

                <td>
                    <img src=@item.imagePath height="30" width="30" />
                </td>

                <td>

                    <a asp-controller="Profiles" asp-action="Details" asp-route-id="@item.ProfileID">Details</a>
                    <a asp-controller="Admin" asp-action="Delete" asp-route-id="@item.ProfileID">Delete</a>



                </td>
            </tr>
        }
    </tbody>
</table>